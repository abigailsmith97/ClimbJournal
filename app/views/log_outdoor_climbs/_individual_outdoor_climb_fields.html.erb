<main>
    <section class="py-5 text-left container">
        <!-- <div class="row py-lg-5">
            <div class="col-lg-6 col-md-8 mx-auto"> -->

        <!-- Nested Form for f -->
        <div class="nested-fields">
            <table class="table table-bordered">
                <tr>
                    <td class="col-md-2">
                        <%= f.label :climb_name, class: "form-label" %>
                        <%= f.text_field :climb_name, class: "form-control" %>
                    </td>
                    <td class="col-md-2">
                        <%= f.label :grade, class: "form-label" %>
                        <%= f.text_field :grade, class: "form-control" %>
                    </td>
                    <td class="col-md-2">
                        <%= f.label :climbing_style, class: "form-label" %>
                        <%= f.text_field :climbing_style, class: "form-control" %>
                    </td>
                    <td class="col-md-2">
                        <%= f.label :climbing_partner, class: "form-label" %>
                        <%= f.text_field :climbing_partner, class: "form-control" %>
                    </td>
                    <td class="col-md-2">
                        <%= f.label :notes, class: "form-label" %>
                        <%= f.text_field :notes, class: "form-control" %>
                    </td>
                    <td class="col-md-2">
                        <%= f.hidden_field :_destroy %>
                        <%= link_to_remove_association "Delete", f, class: "btn btn-danger btn-sm mt-2" %>
                    </td>
                </tr>
            </table>
        </div>
    </section>
</main>


    
